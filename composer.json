{
    "name":              "ocramius/ocramius",
    "description":       "Ocramius is Ocramius - there's nothing more to say.",
    "type":              "library",
    "license":           "MIT",
    "homepage":          "https://github.com/Ocramius/Ocramius",
    "keywords":          [
        "ocramius"
    ],
    "authors": [
        {
            "name":     "Marco Pivetta",
            "email":    "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        },
        {
            "name":     "Luis Cordova",
            "email":    "cordoval@gmail.com",
            "homepage": "http://craftitonline.com/"
        }
    ],
    "require": {
        "php":                          ">=5.5",
        "zendframework/zend-console":   "~2.3",
        "zendframework/zend-validator": "~2.3",
        "zendframework/zend-uri":       "~2.3",
        "zendframework/zend-http": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit":             "~4.0",
